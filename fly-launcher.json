{
  "app": {
      "config": {
          "env": {
              "ELIXIR_ERL_OPTIONS": "-proto_dist inet6_tcp",
              "LIVEBOOK_DATA_PATH": "/data",
              "LIVEBOOK_HOME": "/data",
              "LIVEBOOK_IP": "::",
              "LIVEBOOK_ROOT_PATH": "/data",
              "PORT": "8080"
          },
          "image": "redis",
          "mounts": [
              {
                  "destination": "/data",
                  "source": "livebook_data"
              }
          ],
          "services": [
              {
                  "concurrency": {
                      "hard_limit": 25,
                      "soft_limit": 20,
                      "type": "connections"
                  },
                  "http_checks": [],
                  "internal_port": 6379,
                  "ports": [
                      {
                          "handlers": [
                              "http"
                          ],
                          "port": 6379
                      }
                  ],
                  "protocol": "tcp",
                  "script_checks": [],
                  "tcp_checks": [
                      {
                          "grace_period": "1s",
                          "interval": "15s",
                          "restart_limit": 6,
                          "timeout": "2s"
                      }
                  ]
              }
          ]
      },
      "secrets": [
          {
              "name": "LIVEBOOK_PASSWORD",
              "value": "${livebook_password}"
          },
          {
              "name": "LIVEBOOK_SECRET_KEY_BASE",
              "value": "${livebook_secret_key_base}"
          }
      ],
      "volumes": [
          {
              "name": "livebook_data",
              "size": 1
          }
      ]
  },
  "name": "Redis",
  "description": "Redis is an in-memory data structure store, used as a distributed, in-memory key–value database, cache and message broker, with optional durability.",
  "logo_url": "https://upload.wikimedia.org/wikipedia/commons/6/6b/Redis_Logo.svg",
  "parameters": [
      {
          "generate": 32,
          "help_text": "Save this password in a secure place. You won’t be able to see it again, though you can change it with `flyctl secrets set LIVEBOOK_PASSWORD=...`",
          "label": "LIVEBOOK_PASSWORD",
          "name": "livebook_password",
          "required": true,
          "store_in_cookie": true,
          "type": "string"
      }
  ],
  "post_instructions": "Your new Livebook app is ready to go",
  "template_description": "This template will deploy a Livebook VM with an attached persistent volume.",
  "values": [
      {
          "generate": 64,
          "label": "LIVEBOOK_SECRET_KEY_BASE",
          "name": "livebook_secret_key_base",
          "type": "string"
      }
  ]
}